<UserControl x:Class="CFLMedCab.View.CloseCabinet"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CFLMedCab.View"
      xmlns:cvt="clr-namespace:CFLMedCab.Converter"
      mc:Ignorable="d">
    <Grid Height="260" Width="355">
        <Border  CornerRadius="3,3,3,3" Background="{StaticResource LoginBkBrush}">
            <StackPanel>
                <Image Width="184" Height="120" HorizontalAlignment="Center" Margin="0,35,0,0" >
                    <Image.Source>
                        <BitmapImage UriSource="/CFLMedCab;component/Resources/Images/CloseDoor.png" />
                    </Image.Source>
                </Image>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
