<UserControl x:Class="CFLMedCab.View.Inventory.InventoryOngoing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CFLMedCab.View"
        mc:Ignorable="d"
        Width="960"   Height="540" >
    <Grid Height="245" Width="390">
        <Border  CornerRadius="3,3,3,3" Background="{StaticResource LoginBkBrush}">
            <StackPanel>
                <Image Width="194" Height="128" HorizontalAlignment="Center" Margin="0,48,0,0" >
                    <Image.Source>
                        <BitmapImage UriSource="/CFLMedCab;component/Resources/Images/Inventorying.png" />
                    </Image.Source>
                </Image>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
