<UserControl x:Class="CFLMedCab.View.Inventory.InventoryOngoing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CFLMedCab.View"
        mc:Ignorable="d">
    <Grid Height="489" Width="780">
        <Border  CornerRadius="6,6,6,6" Background="{StaticResource LoginBkBrush}">
            <StackPanel>
                <Image Width="387" Height="255" HorizontalAlignment="Center" Margin="0,96,0,0" >
                    <Image.Source>
                        <BitmapImage UriSource="/CFLMedCab;component/Resources/Images/Inventorying.png" />
                    </Image.Source>
                </Image>
                <Label  HorizontalAlignment="Center" Margin="0,25,0,0"  FontSize="40" Foreground="#619CF8"/>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
