    <Style x:Key="btnScanStyle" TargetType="{x:Type ToggleButton}">
        <Setter Property="Content" Value="扫描"></Setter>
        <Setter Property="Background" Value="Red"></Setter>
        <Style.Triggers>
            <Trigger Property="IsChecked" Value="true" >
