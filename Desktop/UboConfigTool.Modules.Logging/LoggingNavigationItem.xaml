<UserControl x:Class="UboConfigTool.Modules.Logging.LoggingNavigationItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UboConfigTool.Modules.Logging"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <RadioButton x:Name="LoggingButton"
                             Background="Transparent"
                             Tag="/UboConfigTool.Controls;component/Resources/Images/log.png"
                             Click="LoggingButton_Click"
                             Style="{DynamicResource MainNavButtonStyle}"
                             ToolTip="日志管理"/>
    </Grid>
</UserControl>
