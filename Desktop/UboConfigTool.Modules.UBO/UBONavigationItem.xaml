<UserControl x:Class="UboConfigTool.Modules.UBO.UBONavigationItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UboConfigTool.Modules.UBO"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <RadioButton x:Name="UBOButton"
                             Background="Transparent"
                             Tag="/UboConfigTool.Controls;component/Resources/Images/ubo.png"
                             Style="{DynamicResource MainNavButtonStyle}"
                             Click="UBOButton_Click"
                             ToolTip="UBO管理"/>

    </Grid>
</UserControl>
